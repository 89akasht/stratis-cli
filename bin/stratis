#!/usr/bin/env python

"""
Entry script for stratis CLI.
"""

from __future__ import print_function

import os
import sys

from stratis_cli import run

def main():
    """
    The main method.
    """
    execution = run(sys.argv[1:])
    args = next(execution)
    print(args)
    print(os.linesep)
    return next(execution)

if __name__ == "__main__":
    main()
